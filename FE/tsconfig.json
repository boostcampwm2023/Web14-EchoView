{
  "compilerOptions": {
    "target": "es5", // 결과 파일 형식
    "module": "es2015", // module 형식
    "esModuleInterop": true, // import시 namespace alias 가능
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "strict": true,
    "noImplicitAny": true
  },
  "include": ["src"]
}
